        <style>
            {% include "css/main.css" ignore missing %}
        </style>
        <link
        rel="stylesheet"
        href="/css/main.css"
        type="text/css"
/>
        <link
        rel="stylesheet"
        href="//fonts.googleapis.com/css?family=Oxygen|Radley"
        type="text/css"
/>
