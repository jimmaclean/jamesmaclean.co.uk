    <body>

        {% set navPages = collections.all | eleventyNavigation %}
        <nav class="wrap">
            <ul>
                {%- for entry in navPages %}
                    <li>
                        <a href="{{ entry.url | url }}" {% if entry.url == page.url %} class="active"{% endif %}>{{ entry.title }}</a>
                    </li>
                {%- endfor %}
            </ul>
        </nav>

        <div class="wrap">{{ content | safe }}</div>

    </body>

</html>